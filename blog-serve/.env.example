DB_HOST=localhost
DB_PORT=5432
DB_DATABASE=nestjs-blog-serve
DB_USERNAME=root
DB_PASSWORD=123456

# https://github.com/miZyind/nestjs-demo/blob/master/.env.example
# App (src/configs/app.ts)
APP_NAME=blog-serve
APP_HOST=localhost
APP_PORT=3000
APP_SECRET=xxx
APP_SWAGGER=true
APP_JWT_SECRET=xxx
APP_JWT_EXPIRES=604800
# Database (src/configs/database.ts)
TYPEORM_CONNECTION=postgres
TYPEORM_HOST=localhost
TYPEORM_PORT=5432
TYPEORM_USERNAME=root
TYPEORM_PASSWORD=123456
TYPEORM_DATABASE=nestjs-blog-serve
TYPEORM_ENTITIES=dist/entities/*.js
TYPEORM_MIGRATIONS=dist/migrations/*.js
TYPEORM_MIGRATIONS_RUN=false
TYPEORM_LOGGING=true
TYPEORM_SYNCHRONIZE=true
TYPEORM_DROP_SCHEMA=false
# TypeORM CLI
TYPEORM_ENTITIES_DIR=src/entities
TYPEORM_MIGRATIONS_DIR=src/migrations
# RabbitMQ (src/configs/rmq.ts)
RMQ_ENABLE=true
RMQ_URL=amqp://localhost:5672
